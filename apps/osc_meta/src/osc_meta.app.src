{application, osc_meta, [
    {description, "App for handling metric-related metadata"},
    {vsn, git},
    {registered, [osc_meta_sup]},
    {applications, [
        kernel,
        stdlib,
        osc_sql,
        osc_http
    ]},
    {mod, {osc_meta_app, []}},
    {env, [
        {default_window_configuration, [
            {rectangular, average, 10, 1000},
            {rectangular, average, 60, 1000},
            {rectangular, average, 3600, 1000}
        ]}
    ]}
]}.
